{
    "manifest_version": 3,
    "name": "URL Blocker",
    "description": "Block scripts by URL.",
    "version": "1.0",
    "permissions": [
        "declarativeNetRequest",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}
